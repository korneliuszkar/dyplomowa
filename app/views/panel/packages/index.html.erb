<div class="table-responsive">
<table class="table table-bordered">
  <tr>
    <td>Id</td>
    <td>Nazwa</td>
    <td></td>
    <td></td>
    <td></td>
  </tr>
  <% @packages.each do |package| %>
      <tr>
        <td><%= package.id %></td>
        <td><%= package.name %></td>
        <td><%= link_to "show", panel_package_path(package.id) %></td>
        <td><%= link_to "edit", edit_panel_package_path(package) %></td>
        <td><%= link_to "delete", panel_package_path(package), remote: false, method: :delete, data: { confirm: 'Are you certain you want to delete this?' }%></td>
      </tr>
  <% end %>
  <tr>
    <td><%= link_to "new package", new_panel_package_path %></td> 
  </tr>
</table>
</div>